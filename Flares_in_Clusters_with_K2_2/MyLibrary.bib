	Archiveprefix = {arXiv},
	Author = {{Kirkpatrick}, J.~D. and {Looper}, D.~L. and {Burgasser}, A.~J. and {Schurr}, S.~D. and {Cutri}, R.~M. and {Cushing}, M.~C. and {Cruz}, K.~L. and {Sweet}, A.~C. and {Knapp}, G.~R. and {Barman}, T.~S. and {Bochanski}, J.~J. and {Roellig}, T.~L. and {McLean}, I.~S. and {McGovern}, M.~R. and {Rice}, E.~L.},
