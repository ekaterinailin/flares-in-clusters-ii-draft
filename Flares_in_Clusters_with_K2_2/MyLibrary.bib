@article{Aarnio2011,
  title = {Solar {{Flares}} and {{Coronal Mass Ejections}}: {{A~Statistically Determined Flare Flux}} - {{CME Mass Correlation}}},
  volume = {268},
  issn = {1573-093X},
  doi = {10.1007/s11207-010-9672-7},
  abstract = {In an effort to examine the relationship between flare flux and corresponding CME mass, we temporally and spatially correlate all X-ray flares and CMEs in the LASCO and GOES archives from 1996 to 2006. We cross-reference 6733 CMEs having well-measured masses against 12thinspace050 X-ray flares having position information as determined from their optical counterparts. For a given flare, we search in time for CMEs which occur 10thinspace--thinspace80~minutes afterward, and we further require the flare and CME to occur within textpmthinspace45$^\circ$ in position angle on the solar disk. There are 826 CME/flare pairs which fit these criteria. Comparing the flare fluxes with CME masses of these paired events, we find CME mass increases with flare flux, following an approximately log-linear, broken relationship: in the limit of lower flare fluxes, logthinspace(CME mass)$\propto$0.68\texttimes{}logthinspace(flare flux), and in the limit of higher flare fluxes, logthinspace(CME mass)$\propto$0.33\texttimes{}logthinspace(flare flux). We show that this broken power-law, and in particular the flatter slope at higher flare fluxes, may be due to an observational bias against CMEs associated with the most energetic flares: halo CMEs. Correcting for this bias yields a single power-law relationship of the form logthinspace(CME mass)$\propto$0.70\texttimes{}logthinspace(flare flux). This function describes the relationship between CME mass and flare flux over at least 3~dex in flare flux, from $\approx$thinspace10-7thinspace--thinspace10-4~Wthinspacem-2.},
  timestamp = {2018-06-15T16:32:50Z},
  number = {1},
  journal = {\solphys},
  author = {Aarnio, A. N. and Stassun, K. G. and Hughes, W. J. and McGregor, S. L.},
  month = jan,
  year = {2011},
  keywords = {CME,flare,solar-stellar connection,Sun,X-ray},
  pages = {195--212},
  annote = {Key points:

log(CME mass) $\propto$ 0.70 \texttimes{} log(flare flux)
correlating flares (\textasciitilde{}12000 ) and CMEs (\textasciitilde{}7000) on the Sun with known fluxes and masses respectively reveals a single power law in case of spatially associated flares/CMEs
CMEs are faster if associated with flares
Although flares themselves are not the cause of CMEs, their properties could serve as prediction tools for imminent CMEs and particle events, which are of much concern in the field of space weather.
},
  file = {Aarnio_2011_Sol.pdf:/media/ekaterina/OS/Users/Katja/Documents/Astro/Literature/Cool Stars and Magnetic Activity/Aarnio_2011_Sol.pdf:application/pdf},
  groups = {Cool Stars and Magnetic Activity},
  day = {01}
}

