@ARTICLE{mcquillan2014,
       author = {{McQuillan}, A. and {Mazeh}, T. and {Aigrain}, S.},
        title = "{Rotation Periods of 34,030 Kepler Main-sequence Stars: The Full Autocorrelation Sample}",
      journal = {\apjs},
     keywords = {catalogs, methods: data analysis, methods: observational, stars: activity, stars: low-mass, stars: rotation, techniques: photometric, Astrophysics - Solar and Stellar Astrophysics},
         year = 2014,
        month = apr,
       volume = {211},
       number = {2},
          eid = {24},
        pages = {24},
     abstract = "{We analyzed three years of data from the Kepler space mission to derive
        rotation periods of main-sequence stars below 6500 K. Our
        automated autocorrelation-based method detected rotation periods
        between 0.2 and 70 days for 34,030 (25.6\%) of the 133,030 main-
        sequence Kepler targets (excluding known eclipsing binaries and
        Kepler Objects of Interest), making this the largest sample of
        stellar rotation periods to date. In this paper we consider the
        detailed features of the now well-populated period-temperature
        distribution and demonstrate that the period bimodality, first
        seen by McQuillan et al. in the M-dwarf sample, persists to
        higher masses, becoming less visible above 0.6 M $_{☉}$. We show
        that these results are globally consistent with the existing
        ground-based rotation-period data and find that the upper
        envelope of the period distribution is broadly consistent with a
        gyrochronological age of 4.5 Gyr, based on the isochrones of
        Barnes, Mamajek, \&amp; Hillenbrand and Meibom et al. We also
        performed a detailed comparison of our results to those of
        Reinhold et al. and Nielsen et al., who measured rotation
        periods of field stars observed by Kepler. We examined the
        amplitude of periodic variability for the stars with detection
        rotation periods, and found a typical range between
        \raisebox{-0.5ex}\textasciitilde950 ppm (5th percentile) and
        \raisebox{-0.5ex}\textasciitilde22,700 ppm (95th percentile),
        with a median of \raisebox{-0.5ex}\textasciitilde5600 ppm. We
        found typically higher amplitudes for shorter periods and lower
        effective temperatures, with an excess of low-amplitude stars
        above \raisebox{-0.5ex}\textasciitilde5400 K.}",
          doi = {10.1088/0067-0049/211/2/24},
archivePrefix = {arXiv},
       eprint = {1402.5694},
 primaryClass = {astro-ph.SR},
       adsurl = {https://ui.adsabs.harvard.edu/abs/2014ApJS..211...24M},
      adsnote = {Provided by the SAO/NASA Astrophysics Data System}
}



