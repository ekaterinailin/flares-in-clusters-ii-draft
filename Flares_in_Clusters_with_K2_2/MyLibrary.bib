	Keywords = {Brown Dwarfs--Surveys--Stars: activity, flare, low-mass},
	Pages = {741},
	Primaryclass = {astro-ph.SR},
	Title = {{Examining the age/activity relationship of Ultracool Dwarfs with Gaia }},
	Volume = 85,
